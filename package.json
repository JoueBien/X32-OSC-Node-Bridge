  "description": "React-TypeScript-Electron sample with Create React App and Electron Builder",
  "homepage": ".",
  "main": "build/electron/main.js",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^1.5.0",
    "osc": "^2.4.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-scripts": "5.0.1",
    "rsuite": "^5.19.0",
    "serialport": "^10.4.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.3",
    "use-async-setstate": "^0.1.1",
    "use-debounce": "^8.0.4",
    "usehooks-ts": "^2.7.2",
    "uuid": "^9.0.0",
    "validator": "^13.7.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm run build && tsc -p electron && electron-builder",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx,.ts .",
    "fix": "eslint --fix --ext .tsx,.ts ."
  },
  "build": {
    "buildDependenciesFromSource": true,
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@josephmark/eslint-config": "^1.0.2",
    "@josephmark/eslint-config-react": "^1.0.1",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.7",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "electron": "^23.1.2",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.9",
    "prettier": "^2.7.1",
    "wait-on": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
